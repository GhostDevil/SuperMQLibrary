<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
     <OutputType>Library</OutputType>
    <TargetFrameworks>net462;net472;net48;net6.0;net7.0</TargetFrameworks>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <RootNamespace>SuperMQ</RootNamespace>
    <AssemblyName>SuperMQLibrary</AssemblyName>
    <FileVersion>22.11.8.0</FileVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net45|AnyCPU'">
    <DocumentationFile>obj\SuperMQLibrary.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="app.config" />
    <None Remove="SuperMQLibrary.xml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Apache.NMS" Version="2.0.0" />
    <PackageReference Include="Apache.NMS.ActiveMQ" Version="2.0.0" />
    <PackageReference Include="DotPulsar" Version="2.5.2" />
    <PackageReference Include="MQTTnet" Version="4.1.3.436" />
    <PackageReference Include="MQTTnet.Extensions.ManagedClient" Version="4.1.3.436" />
  </ItemGroup>
  <ItemGroup>
    <None Update="ConfigInfo\MQConfig.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net472'">
    <PackageReference Include="RabbitMQ.Client">
      <Version>6.3.0</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net48'">
    <PackageReference Include="RabbitMQ.Client">
      <Version>6.3.0</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net6.0-windows'">
    <PackageReference Include="RabbitMQ.Client">
      <Version>6.3.0</Version>
    </PackageReference>
  </ItemGroup>
 
</Project>